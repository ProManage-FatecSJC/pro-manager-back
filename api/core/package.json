  "scripts": {
    "dev": "nodemon --exec ts-node ./src/server.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migrationIds:generate": "typeorm-ts-node-commonjs -d ./src/data-source-ids.ts migration:generate ./src/migrationsIds/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run ",
    "migrationIds:run": "typeorm-ts-node-commonjs -d ./src/data-source-ids.ts migration:run ",
    "migration:create": "cd ./src/db/migrations && typeorm migration:create"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.1",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@h4ad/serverless-adapter": "^2.16.0",
    "@types/aws-lambda": "^8.10.114",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  }
